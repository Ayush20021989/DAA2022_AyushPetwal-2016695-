#include<stdio.h>
#include<stdlib.h>
int main()
{
    int m,n;
    printf("Enter size of first array:");
    scanf("%d",&m);
    int *arr1=(int *)malloc(m*sizeof(int));
    for(int i=0;i<m;i++)
    scanf("%d",&arr1[i]);
    printf("Enter size of second array:");
    scanf("%d",&n);
      int *arr2=(int *)malloc(n*sizeof(int));
    for(int i=0;i<n;i++)
    scanf("%d",&arr2[i]);
    printf("Common elements are:");
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(arr1[i]==arr2[j])
            printf("%d ",arr1[i]);
        }
    }
}
